{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "description": "Marvel Heroes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build coverage stats.json",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "webpack-dev-server -d --host 0.0.0.0",
    "start:prod": "webpack-dev-server -p --compress --host 0.0.0.0",
    "build": "webpack -d",
    "build:watch": "webpack -d --watch",
    "build:prod": "webpack -p",
    "build:prod:analyze": "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest --config jest-e2e.config.js",
    "tsc": "tsc",
    "format": "prettier --write \"**/*\"",
    "lint": "npm run tsc && eslint . '**/*.{js,jsx,ts,tsx}' && stylelint '**/*.scss'",
    "precommit": "npm run format && npm run lint",
    "prepush": "npm run precommit && npm run test && npm run test:e2e",
    "deploy": "npm run clean && npm run build:prod && aws s3 sync build/ s3://marvelheroes",
    "npm:update": "ncu --upgrade"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@testing-library/react": "^9.4.0",
    "blueimp-md5": "^2.12.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.1",
    "raven-js": "^3.27.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/blueimp-md5": "^2.7.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/expect-puppeteer": "^3.3.3",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.25",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/puppeteer": "^2.0.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.1",
    "@types/sass": "^1.16.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.12.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-check-updates": "^4.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "stylelint": "^12.0.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
