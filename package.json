{
  "name": "marvel-heroes",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/MarvelHeroes.git"
  },
  "description": "Marvel Heroes",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build coverage",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "build": "webpack -d",
    "build:watch": "webpack -d --watch",
    "build:prod": "webpack -p",
    "start": "webpack-dev-server -d --host 0.0.0.0",
    "start:prod": "webpack-dev-server -p --compress --host 0.0.0.0",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest --config jest-e2e.config.js",
    "prettier": "prettier --write \"**/*\"",
    "eslint": "eslint . '**/*.{js,jsx,ts,tsx}'",
    "precommit": "npm run prettier && tsc && npm run eslint",
    "prepush": "npm run precommit && npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@types/blueimp-md5": "^2.7.0",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "blueimp-md5": "^2.10.0",
    "bootstrap": "^4.3.1",
    "raven-js": "^3.27.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/expect-puppeteer": "^3.3.1",
    "@types/jest": "^24.0.9",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/puppeteer": "^1.12.1",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.4.9",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "file-loader": "^3.0.1",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-puppeteer": "^4.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "puppeteer": "^1.12.2",
    "react-test-renderer": "^16.8.3",
    "react-testing-library": "^6.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
