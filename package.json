{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "description": "Marvel Heroes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build coverage stats.json",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "webpack-dev-server -d --host 0.0.0.0",
    "start:prod": "webpack-dev-server -p --compress --host 0.0.0.0",
    "build": "webpack -d",
    "build:watch": "webpack -d --watch",
    "build:prod": "webpack -p",
    "build:prod:analyze": "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "jest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "NODE_ENV=test jest --config jest-e2e.config.js",
    "tsc": "tsc",
    "format": "prettier --write \"**/*\"",
    "lint": "npm run tsc && eslint . '**/*.{js,ts,tsx}' && stylelint '**/*.scss'",
    "prepush": "npm run test && npm run test:e2e",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@sentry/browser": "^5.15.2",
    "@testing-library/react": "^10.0.1",
    "blueimp-md5": "^2.13.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/blueimp-md5": "^2.7.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.4",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sass": "^1.16.0",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.12.1",
    "husky": "^4.2.3",
    "jest": "^25.2.3",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.0.9",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "puppeteer": "^2.1.1",
    "purgecss-webpack-plugin": "^2.1.0",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
